{
  "name": "field-trip",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "format": "lint-staged",
    "huskyinstall": "husky install && rm -rf .git/hooks && ln -s ../.husky .git/hooks",
    "format:all": "prettier --write \"src/**/*.{js,jsx,json,ts,tsx}\"",
    "start": "next start -p $PORT",
    "local": "env-cmd -f ./env/.local next dev -p 3000",
    "dev": "env-cmd -f ./env/.dev next dev -p 3000",
    "dev:proxy": "env-cmd -f ./env/.dev node proxy.server.js",
    "build:dev": "env-cmd -f ./env/.dev next build",
    "build:production": "env-cmd -f ./env/.prod next build && next export"
  },
  "dependencies": {
    "@alfonmga/react-lottie-light-ts": "0.0.1",
    "axios": "0.26.0",
    "babel-plugin-inline-react-svg": "2.0.1",
    "babel-plugin-styled-components": "2.0.2",
    "babel-plugin-transform-remove-console": "6.9.4",
    "dotenv": "16.0.0",
    "env-cmd": "10.1.0",
    "moment": "2.29.1",
    "next": "12.0.10",
    "next-compose-plugins": "2.2.1",
    "next-transpile-modules": "9.0.0",
    "nookies": "2.5.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.27.0",
    "react-is": "17.0.2",
    "react-query": "3.34.15",
    "styled-components": "5.3.3",
    "swiper": "8.0.6",
    "typescript": "4.5.5",
    "urlencoded-body-parser": "3.0.0",
    "zustand": "3.7.0"
  },
  "devDependencies": {
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "cors": "2.8.5",
    "express": "4.17.2",
    "http-proxy-middleware": "2.0.3",
    "husky": "7.0.4",
    "lint-staged": "12.3.4",
    "prettier": "2.5.1",
    "simple-zustand-devtools": "1.0.1"
  },
  "proxy": "http://localhost:8080"
}
